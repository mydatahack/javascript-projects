{
  "name": "react-graphql-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "yarn generate-config-file && webpack-dev-server --open",
    "watch": "generate-config-file && webpack-dev-server --open --watch",
    "devbuild": "generate-config-file && webpack --mode development",
    "build": "generate-config-file && webpack --mode production",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint-css": "stylelint 'src/**/*.{c,sc,le}ss'",
    "fix-css": "printf '\\033[43mNote: You may need to run this again if any of your CSS property names contain uppercase letters.\\033[0m' && stylelint 'src/**/*.{c,sc,le}ss' --fix -f 'verbose'",
    "lint-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet",
    "fix-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix --quiet",
    "check-types": "tsc --noEmit",
    "codegen": "graphql-codegen  -r dotenv/config --config codegen.yml",
    "generate-config-file": "node ./bin/generate.js"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.7",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@uppy/aws-s3-multipart": "^1.5.2",
    "@uppy/core": "^1.7.0",
    "@uppy/react": "^1.4.1",
    "@uppy/xhr-upload": "^1.4.1",
    "css-loader": "^4.2.1",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.2.2",
    "postcss-loader": "^3.0.0",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "stylelint": "^11.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.1",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^4.0.0",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "css-modules-typescript-loader": "^4.0.1",
    "debounce-promise": "^3.1.2",
    "graphql": "^15.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "typescript": "^3.9.7"
  }
}
