{
  "name": "parcel-ts-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "lint-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet",
    "fix-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix --quiet",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "start": "parcel ./src/index.html --open",
    "watch": "parcel watch ./src/index.html",
    "build": "parcel build ./src/index.html -o parcel.html"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "enzyme": "^3.11.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "parcel": "^2.0.0-nightly.643",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-css-modules-type-generator": "^1.2.0",
    "postcss": "^8.2.12",
    "postcss-modules": "3.2.2",
    "sass": "^1.32.11",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  }
