{
  "name": "parcel-ts-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint-css": "stylelint 'src/**/*.{c,sc,le}ss'",
    "fix-css": "printf '\\033[43mNote: You may need to run this again if any of your CSS property names contain uppercase letters.\\033[0m' && stylelint 'src/**/*.{c,sc,le}ss' --fix -f 'verbose'",
    "lint-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet",
    "fix-js": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix --quiet",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "start": "parcel ./src/index.html --open",
    "watch": "parcel watch ./src/index.html",
    "build": "parcel build ./src/index.html"
  },
  "prettier": "eslint-config-mdh-typescript-react/prettier",
  "dependencies": {
    "@mdhnpm/react-cube-loading-spinner": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "autoprefixer": "^10.2.5",
    "enzyme": "^3.11.0",
    "eslint-config-mdh-typescript-react": "^1.3.0",
    "jest": "^26.6.3",
    "parcel": "^2.4.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-css-modules-type-generator": "^1.2.0",
    "postcss": "^8.2.12",
    "postcss-modules": "3.2.2",
    "sass": "^1.32.11",
    "stylelint": "^13.12.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  }
}
