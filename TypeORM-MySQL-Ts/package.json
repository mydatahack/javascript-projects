{
  "name": "TypeORM-MySQL-Ts",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "Mdh",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./ --ext .ts",
    "lint-fixup": "./node_modules/.bin/eslint ./ --ext .ts,.tsx --fix",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "fixtures": "ts-node ./fixtures/index.ts",
    "typeorm-migrate": "yarn typeorm migration:run",
    "start-local": "nodemon --watch '**/*.ts' --ignore '**/*.spec.ts' --exec 'ts-node' ./index.ts",
    "build": "tsc",
    "develop": "tsc -w"
  },
  "dependencies": {
    "apollo-server": "^2.24.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.0.10",
    "mysql": "^2.18.1",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-resolvers": "^1.19.1",
    "apollo-datasource": "^0.9.0",
    "eslint": "^7.26.0",
    "graphql-codegen": "^0.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typescript": "^4.2.4"
